{
  "name": "barber-queue",
  "version": "1.0.0",
  "description": "Indian Barber-Centric Appointment & Queue Web App",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-deps": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": [
    "barber",
    "queue",
    "appointment",
    "booking"
  ],
  "author": "BarberQueue Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.11.0"
  }
}
